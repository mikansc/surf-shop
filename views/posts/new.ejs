<h1>New post page</h1>

<form action="/posts" method="POST">
  <div>
    <input type="text" name="post[title]" id="" placeholder="Title" />
  </div>
  <div>
    <input type="text" name="post[price]" id="" placeholder="Price" />
  </div>
  <div>
    <textarea
      name="post[description]"
      cols="30"
      rows="10"
      placeholder="Description"
    ></textarea>
  </div>
  <div>
    <input type="text" name="post[location]" id="" placeholder="Location" />
  </div>
  <div>
    <input type="submit" />
  </div>
</form>
