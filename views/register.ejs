<% layout('layouts/boilerplate') %>

<form action="/register" method="post">
  <div>
    <label for="username">Username:</label>
    <input type="text" name="username" placeholder="username" id="username" value="<%= username %>" required />
  </div>
  <div>
    <label for="password">Password:</label>
    <input type="password" name="password" placeholder="password" id="password" required />
  </div>
  <div>
    <label for="email">Email:</label>
    <input type="email" name="email" placeholder="email" id="email" value="<%= email %>" required />
  </div>
  <div>
    <label for="image">Image:</label>
    <input type="file" name="image" placeholder="image" id="image" />
  </div>
  <input type="submit" />
</form>